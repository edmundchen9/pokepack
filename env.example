# Discord Bot Configuration
# Get these values from https://discord.com/developers/applications

# Your bot's token (KEEP THIS SECRET!)
DISCORD_TOKEN=YOUR_DISCORD_TOKEN

# Your bot's client ID
DISCORD_CLIENT_ID=YOUR_CLIENT_ID

# Your Discord user ID (for owner-only commands)
BOT_OWNER_ID=your_user_id_here

# Optional: Specific guild ID for development/testing
DISCORD_GUILD_ID=your_guild_id_here

# Pokemon TCG API Configuration
POKEMON_TCG_API_KEY=YOUR_POKEMON_TCG_API_KEY


# Logging Configuration
LOG_LEVEL=info
LOG_TO_FILE=false

# Database Configuration (if using a database)
# DATABASE_URL=your_database_url_here

# Other API Keys (add as needed)
# WEATHER_API_KEY=your_weather_api_key_here
# NEWS_API_KEY=your_news_api_key_here
